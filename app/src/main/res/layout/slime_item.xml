<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textnameSlime"
        android:layout_width="217dp"
        android:layout_height="40dp"
        android:layout_marginStart="12dp"
        android:fontFamily="sans-serif-medium"
        android:textAlignment="center"
        android:textColor="#0587CF"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="108dp"
        android:layout_height="37dp"
        android:layout_marginStart="28dp"
        android:fontFamily="sans-serif-medium"
        android:text="Diet"
        android:textAlignment="center"
        android:textColor="#036398"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.095" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="116dp"
        android:layout_height="35dp"
        android:layout_marginStart="20dp"
        android:fontFamily="sans-serif-medium"
        android:text="Favourite food"
        android:textAlignment="center"
        android:textColor="#036398"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.097" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="116dp"
        android:layout_height="35dp"
        android:fontFamily="sans-serif-medium"
        android:text="Favourite Toy"
        android:textAlignment="center"
        android:textColor="#036398"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.681"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.096" />

    <TextView
        android:id="@+id/texttypeSlime"
        android:layout_width="111dp"
        android:layout_height="39dp"
        android:fontFamily="@font/rubik"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.268"
        app:layout_constraintStart_toEndOf="@+id/textnameSlime"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024" />

    <TextView
        android:id="@+id/textfavtoySlime"
        android:layout_width="114dp"
        android:layout_height="46dp"
        android:fontFamily="@font/rubik"
        android:text="TextView"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.72"
        app:layout_constraintStart_toEndOf="@+id/textfavmealSlime"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:layout_constraintVertical_bias="0.017" />

    <TextView
        android:id="@+id/textdietSlime"
        android:layout_width="113dp"
        android:layout_height="46dp"
        android:layout_marginStart="24dp"
        android:fontFamily="@font/rubik"
        android:text="TextView"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.018" />

    <TextView
        android:id="@+id/textfavmealSlime"
        android:layout_width="115dp"
        android:layout_height="44dp"
        android:layout_marginStart="20dp"
        android:fontFamily="@font/rubik"
        android:text="TextView"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textdietSlime"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:layout_constraintVertical_bias="0.02" />

    <TextView
        android:id="@+id/textplortSlime"
        android:layout_width="167dp"
        android:layout_height="38dp"
        android:layout_marginStart="40dp"
        android:fontFamily="@font/rubik"
        android:text="TextView"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.356" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="167dp"
        android:layout_height="38dp"
        android:layout_marginStart="24dp"
        android:text="Plort"
        android:textAlignment="viewEnd"
        android:textColor="#036398"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.356" />

</androidx.constraintlayout.widget.ConstraintLayout>